<html>
  <head></head>
  <body><t:loop>
    <div class="contentblock">
      <a><t:s>library:=filter(@name,"(.*)_anchor",1)</t:s> </a>
      <dl class="list"><t:loop>
        <dt>
          <t:s>
            book := object(),
            book.id:=@id,
            book.digibib-url:=concat("http://www.digibib.net/",a/@href),
            book.title:=normalize-space(a/deep-text('')),
            book.library:=filter(a/@href, "DB=([^=&]+)", 1)
          </t:s>
        </dt>
        <dd>
          <t:s>
            book.author:=normalize-space(filter(./deep-text(''),'(.*) *- *[0-9]*$', 1)),
            book.year:=normalize-space(filter(./deep-text(''), '[0-9]*$'))
          </t:s>
        </dd>
      </t:loop></dl>
    </div>
  </t:loop></body>
</html>
