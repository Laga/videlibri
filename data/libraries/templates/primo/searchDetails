                 <LI><STRONG>{currentfield := replace(normalize-space(.), "[.]", "")}</STRONG>
                     <SPAN><t:read source="if (./a/contains(@href, '://')) then x'{.} ( {./a/@href} )' else ." var="book.{$currentfield}!"/></SPAN></LI>
               </t:switch></t:loop>
             </UL>
             
